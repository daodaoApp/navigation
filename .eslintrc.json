{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "rules": {
        "no-console":"off" ,
        "jsx-a11y/href-no-hash": "off",
        "react/forbid-prop-types": "off",
        "react/destructuring-assignment": "off",
        "jsx-a11y/label-has-associated-control": "off",
        "no-param-reassign": "off",
        "max-len":["error", 200],
        "no-underscore-dangle":"off"
    }
}